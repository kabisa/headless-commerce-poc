{
  "name": "nextjs-commerce",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "prettier-fix": "prettier --write .",
    "eslint-fix": "eslint --fix ./components ./pages",
    "find:unused": "next-unused",
    "generate": "graphql-codegen",
    "generate:definitions": "node framework/bigcommerce/scripts/generate-definitions.js",
    "jest:test": "jest",
    "jest:watch": "jest --watch",
    "cypress:open": "yarn run cypress open",
    "cypress:test": "cypress run --browser chrome --spec cypress/integration/tests/**/*",
    "cypress:test-ci": "cypress run --browser chrome --headless --record --key c51ae0dc-ad45-4c9e-b6d3-f19d3e11dfc7 --spec cypress/integration/tests/**/*"
  },
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@reach/portal": "^0.11.2",
    "@tailwindcss/typography": "^0.4.1",
    "@vercel/fetch": "^6.1.0",
    "autoprefixer": "^10.2.4",
    "body-scroll-lock": "^3.1.5",
    "bowser": "^2.11.0",
    "classnames": "^2.2.6",
    "cookie": "^0.4.1",
    "dot-object": "^2.1.4",
    "email-validator": "^2.0.4",
    "immutability-helper": "^3.1.1",
    "js-cookie": "^2.2.1",
    "keen-slider": "^5.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "^10.0.9-canary.5",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.11.0",
    "next-themes": "^0.0.4",
    "nextjs-progressbar": "^0.0.11",
    "postcss": "^8.2.6",
    "postcss-nesting": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-merge-refs": "^1.1.0",
    "react-ticker": "^1.2.2",
    "shopify-buy": "^2.11.0",
    "swr": "^0.4.0",
    "tabbable": "^5.1.5",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/schema-ast": "^1.18.1",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@manifoldco/swagger-to-ts": "^2.1.0",
    "@next/bundle-analyzer": "^10.0.1",
    "@testing-library/cypress": "^7.0.6",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/classnames": "^2.2.10",
    "@types/cookie": "^0.4.0",
    "@types/jest": "^26.0.15",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/shopify-buy": "^2.10.5",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "cypress": "^7.5.0",
    "cypress-wait-until": "^1.7.1",
    "deepmerge": "^4.2.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql": "^15.4.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.1",
    "lint-staged": "^10.5.3",
    "next-unused": "^0.0.3",
    "node-fetch": "^2.6.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.0.3",
    "vercel": "^23.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix ./components ./pages"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
